<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FF575757"
    tools:context=".fisyaz"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="-17dp">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FF575757"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:context=".fisyaz">


        <Button
            android:id="@+id/button13"
            android:layout_width="177dp"
            android:layout_height="53dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="32dp"
            android:background="@drawable/arkaplan"
            android:onClick="ileri1"
            android:text="İLERİ"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/fisk"
            app:layout_constraintTop_toBottomOf="@+id/tutar" />

        <Button
            android:id="@+id/button9"
            android:layout_width="177dp"
            android:layout_height="53dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/botton"
            android:onClick="gonder"
            android:text="gönder"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="@+id/button13"
            app:layout_constraintStart_toStartOf="@+id/button13"
            app:layout_constraintTop_toBottomOf="@+id/button13" />

        <EditText
            android:id="@+id/firma"
            android:layout_width="match_parent"
            android:layout_height="71dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/botton1"
            android:ems="10"
            android:hint="FİRMA ADI"
            android:inputType="textPersonName"
            android:textAlignment="center"
            android:textColor="#FF0000"
            android:textColorHint="#FF0000"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/belge" />

        <Spinner
            android:id="@+id/gider"
            android:layout_width="match_parent"
            android:layout_height="71dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/botton1"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Name"
            android:textAlignment="center"
            android:textColor="#FF0000"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/kisi1" />

        <EditText
            android:id="@+id/belge"
            android:layout_width="match_parent"
            android:layout_height="71dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/botton1"
            android:ems="10"
            android:hint="BELGE NO"
            android:inputType="textPersonName"
            android:textAlignment="center"
            android:textColor="#FF0000"
            android:textColorHint="#FF0000"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/fistarihi" />

        <EditText
            android:id="@+id/kisi1"
            android:layout_width="match_parent"
            android:layout_height="71dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/botton1"
            android:ems="10"
            android:hint="KİŞİ SAYISI"
            android:inputType="textPersonName"
            android:textAlignment="center"
            android:textColor="#FF0000"
            android:textColorHint="#FF0000"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/firma" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="354dp"
            android:layout_height="52dp"
            android:background="@drawable/botton1"
            android:text="FİŞ EKRANI"
            android:textAlignment="center"
            android:textColor="#FF0000"
            android:textSize="34sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.491"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/fistarihi"
            android:layout_width="match_parent"
            android:layout_height="71dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/botton1"
            android:ems="10"
            android:hint="FİŞ TARİHİ"
            android:inputType="date"
            android:textAlignment="center"
            android:textColor="#FF0000"
            android:textColorHint="#FF0000"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView2" />

        <EditText
            android:id="@+id/tutar"
            android:layout_width="match_parent"
            android:layout_height="71dp"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="80dp"
            android:background="@drawable/botton1"
            android:ems="10"
            android:hint="FİŞ TUTARI"
            android:inputType="numberDecimal"
            android:textAlignment="center"
            android:textColor="#FF0000"
            android:textColorHint="#FF0000"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/gider"
            app:layout_constraintVertical_bias="0.037" />

        <TextView
            android:id="@+id/fisk"
            android:layout_width="209dp"
            android:layout_height="55dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="2dp"
            android:text="1.Fisi Yaziyorsun"
            android:textAlignment="center"
            android:textColor="#F40202"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toStartOf="@+id/button13"
            app:layout_constraintStart_toStartOf="@+id/tutar"
            app:layout_constraintTop_toBottomOf="@+id/tutar" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>